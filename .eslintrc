{
  "extends": [
    "eslint:recommended",
    "plugin:jest/style",
    "plugin:jest/recommended",
    "airbnb-base",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["jest", "prettier"],
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "import/prefer-default-export": "off",
    "import/extensions":["error", {"mjs":  "always"}]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  }
}
